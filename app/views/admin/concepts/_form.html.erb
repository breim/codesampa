<%= bootstrap_form_for([:admin, @concept]) do |f| %>
  <div class="field">
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.text_field :fiat %>
  </div>
  <div class="field">
    <%= f.collection_select :course_id, Course.all, :id, :name, prompt: true, required: true %>
  </div>
  <div class="actions">
    <%= f.submit class: 'btn btn-primary' %>
  </div>
<% end %>
